<%= simple_form_for(@collection_point) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :address %>
    <%= f.input :name %>
    <%= f.input :google_address %>
    <%= f.input :complement %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
